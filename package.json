{
  "name": "match-frontend",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "homepage": "/staking",
  "repository": "git@github.com:Ankr-network/match-frontend.git",
  "buildFolderName": "build",
  "scripts": {
    "build:dev": "tsc --noEmit && vite build --mode dev",
    "build:stage": "tsc --noEmit && vite build --mode stage",
    "build:prod": "tsc --noEmit && vite build --mode prod",
    "start": "yarn start:dev",
    "start:dev": "vite --mode dev",
    "start:stage": "vite --mode stage",
    "start:prod": "vite --mode prod",
    "start:dev:eslint-disabled": "DISABLE_ESLINT=true vite --mode dev",
    "start:stage:eslint-disabled": "DISABLE_ESLINT=true vite --mode stage",
    "start:prod:eslint-disabled": "DISABLE_ESLINT=true vite --mode prod",
    "types": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint --ext .tsx,.ts . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\" && eslint --fix --ext .tsx,.ts src/"
  },
  "dependencies": {
    "@ankr.com/provider": "5.6.1",
    "@ankr.com/utils": "1.3.8",
    "@binance-chain/bsc-connector": "1.0.0",
    "@coinbase/wallet-sdk": "3.9.1",
    "@emotion/cache": "11.13.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/material": "5.16.7",
    "@mui/system": "5.16.7",
    "@mui/utils": "5.16.6",
    "@reduxjs/toolkit": "2.3.0",
    "@walletconnect/ethereum-provider": "2.14.0",
    "axios": "1.7.7",
    "bignumber.js": "9.1.2",
    "date-fns": "^4.1.0",
    "notistack": "3.0.1",
    "p-queue": "8.1.0",
    "pinata-web3": "^0.5.4",
    "react": "18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.1",
    "react-intl-universal": "2.11.3",
    "react-redux": "9.1.2",
    "react-router-dom": "6.27.0",
    "react-virtuoso": "^4.12.5",
    "redux": "5.0.1",
    "tss-react": "4.9.13",
    "use-query-params": "^2.2.1",
    "web3": "4.15.0"
  },
  "devDependencies": {
    "@ankr.com/eslint-config": "^0.0.2",
    "@ankr.com/tsconfig": "^0.0.0",
    "@types/react": "18.3.12",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "18.3.1",
    "@types/react-image-gallery": "^1.2.4",
    "@vitejs/plugin-react": "4.3.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "lint-staged": "^12.1.2",
    "prettier": "3.3.3",
    "typescript": "5.4.5",
    "vite": "6.0.11",
    "vite-plugin-ejs": "1.7.0",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "4.3.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.17"
}
